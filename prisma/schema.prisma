generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String  @id @unique @default(uuid())
  firstName String
  lastName  String
  address   String
  email     String  @unique
  phone     String?
  password  String

  @@map("users")
}
